amf:
  sbi:
    server:
      - address: {{ control_ip }}
        port: 7777
    client:
      scp:
        - uri: http://{{ control_ip }}:7777
      nrf:
        - uri: http://{{ control_ip }}:7777

  ngap:
    server:
      - address: {{ control_ip }}

  metrics:
    server:
      - address: {{ control_ip }}
        port: 9090

  guami:
    - plmn_id:
        mcc: {{ mcc }}
        mnc: {{ mnc }}
      amf_id:
        region: 2
        set: 1

  tai:
    - plmn_id:
        mcc: {{ mcc }}
        mnc: {{ mnc }}
      tac: {{ tac }}

  plmn_support:
    - plmn_id:
        mcc: {{ mcc }}
        mnc: {{ mnc }}
      s_nssai:
        - sst: 1
          sd: 000001
        - sst: 2
          sd: 000002

  security:
    integrity_order: [NIA2, NIA1, NIA0]
    ciphering_order: [NEA0, NEA1, NEA2]

  network_name:
    full: Open5GS

  amf_name: open5gs-amf0
