mme:
  freeDiameter: /etc/freeDiameter/mme.conf
  s1ap:
    server:
      - address: {{ control_ip }}
  gtpc:
    server:
      - address: {{ control_ip }}
  metrics:
    server:
      - address: {{ control_ip }}
        port: 9090
  gummei:
    - plmn_id:
        mcc: {{ mcc }}
        mnc: {{ mnc }}
      mme_gid: 2
      mme_code: 1
  tai:
    - plmn_id:
        mcc: {{ mcc }}
        mnc: {{ mnc }}
      tac: {{ tac }}
  security:
    integrity_order: [EIA2, EIA1, EIA0]
    ciphering_order: [EEA0, EEA1, EEA2]
  network_name:
    full: Open5GS
  mme_name: open5gs-mme0
